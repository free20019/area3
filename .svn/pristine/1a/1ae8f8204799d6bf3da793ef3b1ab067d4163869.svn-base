	function dlwzmany(obj){
		if(obj==0||obj==360){
			return "正北";
		}else if(obj==90){
			return "正东";
		}else if(obj==180){
			return "正南";
		}else if(obj==270){
			return "正西";
		}else if(obj>0&&obj<90){
			return "东北";
		}else if(obj>90&&obj<180){
			return "东南";
		}else if(obj>180&&obj<270){
			return "西南";
		}else if(obj>270&&obj<360){
			return "西北";
		}
	}
