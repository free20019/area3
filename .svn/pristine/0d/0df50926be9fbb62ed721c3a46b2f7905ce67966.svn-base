<%@ page language="java" import="java.util.*" pageEncoding="utf-8"
contentType="text/html; charset=UTF-8" %>
<script src="js/echarts/echarts.js"></script>
<script src="js/echarts/bar.js"></script>
<script src="js/echarts/line.js"></script>
<!-- widget grid -->
<div class="row" style="height:600px;overflow: scroll">
	<!-- row -->
<table border="1" style="width: 102%;">
	<tr style="height: ;overflow: auto;">
		<td>
			<div class="jarviswidget jarviswidget-color-darken" id="wid-id-2" data-widget-editbutton="false">
			<header style = "text-align:left;">
					<span class="widget-icon"> <i class="fa fa-table"></i> </span>
					<h2 id="cymxstj">杭州出租车春运期间分布情况(每小时统计)</h2>
				</header>
				<!-- widget div-->
				<div>
					<div class="jarviswidget-editbox">
					</div>
					<div class="widget-body no-padding">
					<form action="" id="msg-form" class="smart-form" novalidate="novalidate" >
					<section style="width:180px;float:left;">
										<label class="input"> <i class="icon-append fa fa-calendar"></i>
											<input id="ynmxstime" type="text" onClick="WdatePicker({dateFmt:'yyyy-MM-dd'})()" placeholder="--请选择时间--"/>
										</label>
								</section>
								<a href="javascript:void(0);"  class="btn btn-primary"  onclick="findynmxs();">查   询</a>
								<a href="javascript:void(1);"  class="btn btn-primary"  onclick="findynmxsexcle();">导   出</a><span id="ynmxsdaochu"></span>
						</form>
						<table id="ynmxstable" style="float:left;"  class="table table-striped table-bordered table-hover">
							<thead>
								<tr>
									<th>序号</th>
									<th>时间</th>
									<th>杭州市</th>
									<th>占比</th>
									<th>大杭州</th>
									<th>占比</th>
									<th>浙江省</th>
									<th>占比</th>
									<th>河南省</th>
									<th>占比</th>
									<th>安徽省</th>
									<th>占比</th>
									<th>江西省</th>
									<th>占比</th>
									<th>车辆总数</th>
								</tr>
							</thead>
							<tbody id="ynmxstbody" >
							</tbody>
						</table>

					</div>
				</div>
				<div id="hourfxtb" class="chart has-legend"></div>
			</div>
		</td>
	</tr>
</table>
</div>
<script type="text/javascript">
	
	// DO NOT REMOVE : GLOBAL FUNCTIONS!
	pageSetUp();
	var myDate = new Date();
	var a=myDate.getFullYear();
	var b=myDate.getMonth()+1+"";
	if(b.length<2){
		b="0"+b;
	}
	var day = myDate.getDate();
	if(day.length<2){
		day="0"+day;
	}
	$("#ynmxstime").val(a+"-"+b+"-"+day);
	findhourfx();
	//loadDataTableScripts_ynmxstable();
	//loadDataTableScripts_area();
	// PAGE RELATED SCRIPTS
	
	
function findhourfx(){
$.blockUI({message:"<div style='padding:5px;'><h3><font color='blue'>数据查询中,请稍候...</font></h3><img src='img/ajax-loader.gif' border='0'/><br/><br/></div>"});
	$.ajax({
			url : 'findhourfx.action',
			type : 'post',
			data:{
				"time" : $("#ynmxstime").val()
			},
			dataType: 'json',
			timeout : 180000,
			success:function(json){
				var fx = json.list;
				var tab="";
				var i=0;
				var hangzhou=[];
				var dahangzhou=[];
				var zhejiang=[];
				var henan=[];
				var anhui=[];
				var jiangxi=[];
				var time=[];
				//$('#nolinetable').dataTable().fnDestroy(); 
				if(fx!=null){
					for(i=0;i<fx.length;i++){
						time.push(fx[i].time.substr(11,2));
						hangzhou.push((fx[i].hangzhou/100*9612).toFixed(0));
						dahangzhou.push((fx[i].dahangzhou/100*9612).toFixed(0));
						zhejiang.push((fx[i].zhejiang/100*9612).toFixed(0));
						henan.push((fx[i].henan/100*9612).toFixed(0));
						anhui.push((fx[i].anhui/100*9612).toFixed(0));
						jiangxi.push((fx[i].jiangxi/100*9612).toFixed(0));
						tab=tab+"<tr><td nowrap='nowrap'>"+(i+1)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].time+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].hangzhou/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].hangzhou+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].dahangzhou/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].dahangzhou+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].zhejiang/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].zhejiang+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].henan/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].henan+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].anhui/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].anhui+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+(fx[i].jiangxi/100*9612).toFixed(0)+"</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;"+fx[i].jiangxi+"%</td>";
						tab=tab+"<td nowrap='nowrap'>&nbsp;9612</td></tr>";
					}
						$("#ynmxstbody").html(tab);
						hourfxtb(time,hangzhou,dahangzhou,zhejiang,henan,anhui,jiangxi);
	$.unblockUI();
				}
			},
			error:function(){
				
			}		
	});
}
function hourfxtb(time,hangzhou,dahangzhou,zhejiang,henan,anhui,jiangxi){
	  // 使用
 require(
    [
        'echarts',
        'echarts/chart/line', // 
			'echarts/chart/bar' // 使用柱状图就加载bar模块，按需加载
    ],
    function (ec) {
        // 基于准备好的dom，初始化echarts图表
        var myChart = ec.init(document.getElementById('hourfxtb')); 
		
      option = {
      grid:{
				x:40,y:10,x2:10,y2:30
			},
		    title : {
		        text: '',
		        subtext: ''
		    },
		    tooltip : {
		        trigger: 'axis'
		    },
		    legend: {
		        data:['杭州市','大杭州','浙江省','河南省','安徽省','江西省']
		    },
		    toolbox: {
		        show : false,
		       
		    },
		    calculable : true,
		    xAxis : [
		        {
		            type : 'category',
		            boundaryGap : false,
		            data : time
		        }
		    ],
		    yAxis : [
		        {
		            type : 'value',
		            axisLabel : {
		                formatter: '{value}'
		            }
		        }
		    ],
		    series : [
		        {
		            name:'杭州市',
		            type:'line',
		            data:hangzhou,
		        },
		        {
		            name:'大杭州',
		            type:'line',
		            data:dahangzhou,
		        },
		        {
		            name:'浙江省',
		            type:'line',
		            data:zhejiang,
		        },
		        {
		            name:'河南省',
		            type:'line',
		            data:henan,
		        },
		        {
		            name:'安徽省',
		            type:'line',
		            data:anhui,
		        },
		        {
		            name:'江西省',
		            type:'line',
		            data:jiangxi,
		        }
		    ]
		};

        // 为echarts对象加载数据 
        myChart.setOption(option); 
    }
);
}

//导出excle
	function findynmxsexcle(){
	$.blockUI({message:"<div style='padding:5px;'><h3><font color='blue'>数据查询中,请稍候...</font></h3><img src='img/ajax-loader.gif' border='0'/><br/><br/></div>"});
	if(window.confirm("确定按上述条件把数据集导成Excel？")){
		$.ajax({
		url : 'findhourfxexcle.action',
		type : 'post',
		data:{
			"time" : $("#ynmxstime").val()
		},
		dataType: 'json',
		timeout : 180000,
		success:function(json){
			window.location.href=json.action;
			$.unblockUI();
		},
		error:function(){
			
		}
		});
	}
}
$("#cymxstj").html("杭州出租车春运期间分布情况(每小时统计)");
//树
	$('.tree > ul').attr('role', 'tree').find('ul').attr('role', 'group');
	$('.tree').find('li:has(ul)').addClass('parent_li').attr('role', 'treeitem').find(' > span').attr('title', 'Collapse this branch').on('click', function(e) {
		var children = $(this).parent('li.parent_li').find(' > ul > li');
		if (children.is(':visible')) {
			children.hide('fast');
			$(this).attr('title', 'Expand this branch').find(' > i').removeClass().addClass('fa fa-lg fa-plus-circle');
		} else {
			children.show('fast');
			$(this).attr('title', 'Collapse this branch').find(' > i').removeClass().addClass('fa fa-lg fa-minus-circle');
		}
		e.stopPropagation();
	});
		
	if($('.DTTT_dropdown.dropdown-menu').length){
		$('.DTTT_dropdown.dropdown-menu').remove();
	}

	var ynmxstables="";
	function loadDataTableScripts_ynmxstable() {

		loadScript("js/plugin/datatables/jquery.dataTables-cust.min.js", dt_2);

		function dt_2() {
			loadScript("js/plugin/datatables/ColReorder.min.js", dt_3);
		}

		function dt_3() {
			loadScript("js/plugin/datatables/FixedColumns.min.js", dt_4);
		}

		function dt_4() {
			loadScript("js/plugin/datatables/ColVis.min.js", dt_5);
		}

		function dt_5() {
			loadScript("js/plugin/datatables/ZeroClipboard.js", dt_6);
		}

		function dt_6() {
			loadScript("js/plugin/datatables/media/js/TableTools.min.js", dt_7);
		}

		function dt_7() {
			loadScript("js/plugin/datatables/DT_bootstrap.js", runDataTables_ynmxstable);
		}

	}

	function runDataTables_ynmxstable() {

		/*
		 * BASIC
		 */
		ynmxstables=$('#ynmxstable').dataTable({
			"sPaginationType" : "bootstrap_full",
			"bRetrieve": true
		});

		/* END BASIC */

		/* Add the events etc before DataTables hides a column */
		$("#datatable_fixed_column thead input").keyup(function() {
			oTable.fnFilter(this.value, oTable.oApi._fnVisibleToColumnIndex(oTable.fnSettings(), $("thead input").index(this)));
		});

		$("#datatable_fixed_column thead input").each(function(i) {
			this.initVal = this.value;
		});
		$("#datatable_fixed_column thead input").focus(function() {
			if (this.className == "search_init") {
				this.className = "";
				this.value = "";
			}
		});
		$("#datatable_fixed_column thead input").blur(function(i) {
			if (this.value == "") {
				this.className = "search_init";
				this.value = this.initVal;
			}
		});		
		

		var oTable = $('#datatable_fixed_column').dataTable({
			"sDom" : "<'dt-top-row'><'dt-wrapper't><'dt-row dt-bottom-row'<'row'<'col-sm-6'i><'col-sm-6 text-right'p>>",
			//"sDom" : "t<'row dt-wrapper'<'col-sm-6'i><'dt-row dt-bottom-row'<'row'<'col-sm-6'i><'col-sm-6 text-right'>>",
			"oLanguage" : {
				"sSearch" : "Search all columns:"
			},
			"bSortCellsTop" : true
		});		
		


		/*
		 * COL ORDER
		 */
		$('#datatable_col_reorder').dataTable({
			"sPaginationType" : "bootstrap",
			"sDom" : "R<'dt-top-row'Clf>r<'dt-wrapper't><'dt-row dt-bottom-row'<'row'<'col-sm-6'i><'col-sm-6 text-right'p>>",
			"fnInitComplete" : function(oSettings, json) {
				$('.ColVis_Button').addClass('btn btn-default btn-sm').html('Columns <i class="icon-arrow-down"></i>');
			}
		});
		
		/* END COL ORDER */

		/* TABLE TOOLS */
		$('#datatable_tabletools').dataTable({
			"sDom" : "<'dt-top-row'Tlf>r<'dt-wrapper't><'dt-row dt-bottom-row'<'row'<'col-sm-6'i><'col-sm-6 text-right'p>>",
			"oTableTools" : {
				"aButtons" : ["copy", "print", {
					"sExtends" : "collection",
					"sButtonText" : 'Save <span class="caret" />',
					"aButtons" : ["csv", "xls", "pdf"]
				}],
				"sSwfPath" : "js/plugin/datatables/media/swf/copy_csv_xls_pdf.swf"
			},
			"fnInitComplete" : function(oSettings, json) {
				$(this).closest('#dt_table_tools_wrapper').find('.DTTT.btn-group').addClass('table_tools_group').children('a.btn').each(function() {
					$(this).addClass('btn-sm btn-default');
				});
			}
		});
		
		/* END TABLE TOOLS */
	}
</script>
